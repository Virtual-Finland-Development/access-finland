{
  "extends": "tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@pages/*": ["pages/*"],
      "@shared/*": ["../../../packages/vf-shared/src/*"],
      /* "@/*": [
        "../../../packages/vf-shared/src/*"
      ] */
      "@/*": ["./*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
