{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "globalEnv": [
    "ANALYZE",
    "NEXT_PUBLIC_AUTH_GW_BASE_URL",
    "NEXT_PUBLIC_PRH_MOCK_BASE_URL",
    "NEXT_PUBLIC_TESTBED_API_BASE_URL",
    "NEXT_PUBLIC_CODESETS_BASE_URL",
    "NEXT_PUBLIC_USERS_API_BASE_URL",
    "NEXT_PUBLIC_IS_EXPORT",
    "NODE_ENV",
    "NEXT_PUBLIC_STAGE",
    "BACKEND_SECRET_SIGN_KEY",
    "FRONTEND_ORIGIN_URI",
    "JMF_SKILL_RECOMMENDATIONS_ENDPOINT",
    "DATASPACE_PRODUCT_GATEWAY_BASE_URL",
    "DATASPACE_DEFAULT_DATA_SOURCE",
    "WAF_SHARED_COOKIE_SECRET",
    "WAF_USER_POOL_ID",
    "WAF_USER_POOL_CLIENT_ID",
    "USERS_API_ACCESS_KEY",
    "DATASPACE_DEFAULT_SCHEMA_VERSION"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "build:features": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "build:mvp": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "start": {},
    "start:mvp": {},
    "test": {},
    "test:watch": {
      "cache": false
    },
    "test:ci": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": [
        "build"
      ]
    },
    "lint": {},
    "check-types": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}